    <%= link_to "Edit", edit_document_path(@document), class: "btn btn-outline-primary", type: "button"%>
  </div>
  <div class="row">
    <%= @document.name %>
  </div>
  <div class="row">
    <% @document.document_blocks.each do |block| %>
      <%= render partial: "document_blocks/show", locals: {block:  block } %>
    <% end %>
  </div>
</div>
